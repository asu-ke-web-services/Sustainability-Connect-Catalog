{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0.0",
    "backpack/crud": "^3.4",
    "backpack/permissionmanager": "^3.12",
    "backpack/settings": "^2.1",
    "barryvdh/laravel-elfinder": "^0.3.12",
    "fideloper/proxy": "~3.3",
    "infyomlabs/laravel-generator": "5.5.x-dev",
    "infyomlabs/adminlte-templates": "5.5.x-dev",
    "laravel/framework": "5.5.*",
    "laravel/tinker": "~1.0",
    "laravelcollective/html": "^5.5.0",
    "mccool/laravel-auto-presenter": "^7.0",
    "richan-fongdasen/eloquent-blameable": "^1.1",
    "roave/security-advisories": "dev-master",
    "spatie/laravel-sluggable": "^2.1",
    "venturecraft/revisionable": "1.*"
  },
  "require-dev": {
    "backpack/generators": "^1.1",
    "barryvdh/laravel-ide-helper": "^2.4",
    "codeception/codeception": "^2.4",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "laracasts/generators": "dev-master",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0",
    "symfony/thanks": "^1.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "SCCatalog\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
